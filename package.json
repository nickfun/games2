{
  "name": "games2",
  "version": "0.1.0",
  "description": "My videogame collection",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "fire": "rm -rf dist/ ; rm -rf node_modules; npm install",
    "dev-server": "webpack-dev-server",
    "build": "webpack src/index.js dist/out.js --module-bind 'js=babel-loader' ; cp ./dist/out.js ./public/games2/out.js",
    "xbuild": "webpack -p --progress"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/nickfun/games2.git"
  },
  "keywords": [
    "Catalog",
    "Video Games"
  ],
  "author": "Nick Fun",
  "license": "ISC",
  "dependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-react": "6.16.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "redux": "3.6.0",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2"
  },
  "devDependencies": {}
}
