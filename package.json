{
    "name": "games2",
    "version": "0.1.0",
    "description": "My videogame collection",
    "main": "src/index.js",
    "scripts": {
	"clean": "rm -rf dist/*; rm -rf node_modules/*",
	"dev-server": "webpack-dev-server",
	"prebuild": "npm run clean && npm install",
	"build": "webpack -p --progress",
	"postbuild": "git add --all",
	"postversion": "git push --tags",
	"lint": "eslint src/ --ext .jsx --ext .js",
	"lint:fix": "eslint src/ --ext .jsx --ext .js --fix",
	"local:copy": "mkdir -p ~/Projects/php-core/www/console/components/console-js && cp -r ~/Projects/console-js/dist/ ~/Projects/php-core/www/console/components/console-js/"
    },
    "repository": {
	"type": "git",
	"url": "git@github.com/nickfun/games2.git"
    },
    "keywords": [
	"Catalog",
	"Video Games"
    ],
    "author": "Nick Fun",
    "license": "ISC",
    "dependencies": {
	"json-loader": "^0.5.4",
	"lodash": "^4.12.0",
	"moment": "^2.13.0",
	"react": "15.1.0",
	"react-dom": "15.1.0",
	"react-redux": "^4.4.5",
	"redux": "^3.5.2"
    },
    "devDependencies": {
	"assets-webpack-plugin": "^3.4.0",
	"babel-core": "^6.8.0",
	"babel-eslint": "^6.0.4",
	"babel-loader": "^6.2.4",
	"babel-preset-es2015": "^6.6.0",
	"babel-preset-react": "^6.5.0",
	"eslint": "^2.9.0",
	"eslint-plugin-react": "^5.2.2",
	"expose-loader": "^0.7.1",
	"json-loader": "^0.5.4",
	"webpack": "^1.13.0",
	"webpack-dev-server": "^1.14.1"
    }
}
